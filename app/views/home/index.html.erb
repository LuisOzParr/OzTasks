<h1>Hola mundo</h1>
<% @users.each do |user|  %>
  <%= user.name %>
<% end %>

<table>
  <tbody>
    <% @users.each do |user|   %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= image_tag(user.avatar) %></td>
      </tr>
    <% end %>
  </tbody>
</table>